{
  "env": {
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/extensions": [".js", ".jsx"],
    "jsx-a11y/accessible-emoji": false,
    "jsx-a11y/alt-text": false,
    "jsx-a11y/anchor-has-content": false,
    "jsx-a11y/anchor-is-valid": false,
    "jsx-a11y/aria-activedescendant-has-tabindex": false,
    "jsx-a11y/aria-props": false,
    "jsx-a11y/aria-proptypes": false,
    "jsx-a11y/aria-role": false,
    "jsx-a11y/aria-unsupported-elements": false,
    "jsx-a11y/click-events-have-key-events": false,
    "jsx-a11y/heading-has-content": false,
    "jsx-a11y/html-has-lang": false,
    "jsx-a11y/iframe-has-title": false,
    "jsx-a11y/img-redundant-alt": false,
    "jsx-a11y/interactive-supports-focus": false,
    "jsx-a11y/label-has-associated-control": false,
    "jsx-a11y/label-has-for": false,
    "jsx-a11y/media-has-caption": false,
    "jsx-a11y/mouse-events-have-key-events": false,
    "jsx-a11y/no-access-key": false,
    "jsx-a11y/no-autofocus": false,
    "jsx-a11y/no-distracting-elements": false,
    "jsx-a11y/no-interactive-element-to-noninteractive-role": false,
    "jsx-a11y/no-noninteractive-element-interactions": false,
    "jsx-a11y/no-noninteractive-element-to-interactive-role": false,
    "jsx-a11y/no-noninteractive-tabindex": false,
    "jsx-a11y/no-onchange": false,
    "jsx-a11y/no-redundant-roles": false,
    "jsx-a11y/no-static-element-interactions": false,
    "jsx-a11y/role-has-required-aria-props": false,
    "jsx-a11y/role-supports-aria-props": false,
    "jsx-a11y/scope": false,
    "jsx-a11y/tabindex-no-positive": false,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/destructuring-assignment": false,
    "react/prefer-stateless-function": [1, { "ignorePureComponents": true }],
    "no-class-assign": 0,
    "prefer-destructuring": 0,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
